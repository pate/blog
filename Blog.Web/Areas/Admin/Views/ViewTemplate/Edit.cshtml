@model ViewTemplateInputModel

@{
    ViewBag.PageTitle = Model.ViewPath + "Update View Template";
}

<div class="page-header">
    <h1>Edit Template</h1>
</div>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(false)
    @Html.EditorForModel()

    <div class="form-actions">
        <button type="submit" class="btn btn-primary"><i class="icon-save"></i> Update Template</button>
    </div>
}