@model IEnumerable<ViewTemplate>

@{
    ViewBag.PageTitle = "Index";
}

<div class="page-header">
    <h1>View Templates</h1>
</div>

<a class="btn btn-primary" href="@Url.Action("Create")">Create Template</a>

@foreach (var view in Model)
{
    <a href="@Url.Action("Edit", new { id = view.Id })">Edit</a>
    @Html.DisplayFor(x => view)
}